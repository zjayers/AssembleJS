{
  "name": "www.assemblejs.com",
  "version": "0.1.0",
  "private": true,
  "homepage": "/",
  "dependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.8.4",
    "lottie-react": "^2.4.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^9.16.0",
    "react-markdown": "^9.1.0",
    "react-router-dom": "^6.30.0",
    "react-scripts": "5.0.1",
    "react-syntax-highlighter": "^15.6.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "prestart": "cp -r src/docs public/",
    "start": "react-scripts start",
    "build": "react-scripts build && cp -r src/docs build/",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "cp build/index.html build/404.html && echo 'Website built and ready for deployment'",
    "lint": "eslint --max-warnings=0 src",
    "build:ci": "CI=true npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "react-hooks/exhaustive-deps": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
